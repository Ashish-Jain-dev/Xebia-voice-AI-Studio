{
  "name": "xebia-voice-ai-studio",
  "version": "1.0.0",
  "description": "Voice AI platform for Xebia with RAG capabilities",
  "private": true,
  "workspaces": [
    "backend",
    "voice_agent",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:agent\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload --port 8000",
    "dev:agent": "cd voice_agent && python worker.py dev",
    "dev:frontend": "cd frontend && npm run dev",
    "test": "pytest backend/tests/",
    "lint": "pylint backend/ voice_agent/"
  },
  "keywords": [
    "voice-ai",
    "rag",
    "livekit",
    "gemini",
    "fastapi"
  ],
  "author": "Xebia Hackathon Team",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.13.2"
  }
}
